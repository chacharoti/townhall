.container.settings
  .header
    .icon.history
    History

  .row
    - @questions.each do |question|
      .col-xs-6.color-info= link_to question.title, organization_question_result_path(question)
      .col-xs-3= question.organization.name
      .col-xs-3= "Ends #{question.end_date.strftime('%d %b,%Y ')}"
