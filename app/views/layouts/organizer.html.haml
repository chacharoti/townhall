!!!
%html
  %head
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}/
    %title= content_for?(:title) ? yield(:title) : "Townhall Social"
    %meta{:content => content_for?(:description) ? yield(:description) : "Townhall Social", :name => "description"}/
    = stylesheet_link_tag 'organizer', media: 'all', 'data-turbolinks-track' => false
    = javascript_include_tag 'organizer'
    = csrf_meta_tags
  %body
    %header.navbar.navbar-fixed-top.bg-light
      .navbar-branding.dark
        =link_to organizer_dashboard_index_url, class: 'navbar-brand' do 
          %b Townhall Social
      %ul.nav.navbar-nav.navbar-right
        %li.dropdown.dropdown-item-slide
          %a.dropdown-toggle.pl10.pr10{"data-toggle" => "dropdown", :href => "#"}
            %span.octicon.octicon-radio-tower.fs18
          %ul.dropdown-menu.dropdown-hover.dropdown-persist.pn.w350.bg-white.animated.animated-shorter.fadeIn{:role => "menu"}
            %li.bg-light.p8
              %span.fw600.pl5.lh30 Notifications
              %span.label.label-warning.label-sm.pull-right.lh20.h-20.mt5.mr5 12
            %li.p10.br-t.item-1
              .media
                %a.media-left{:href => "#"}
                  %img.mw40{:alt => "holder-img", :src => "assets/img/avatars/2.jpg"}/
                .media-body.va-m
                  %h5.media-heading.mv5
                    Article
                    %small.text-muted - 08/16/22
                  Last Updated 36 days ago by
                  %a.text-system{:href => "#"} Max
            %li.p10.br-t.item-2
              .media
                %a.media-left{:href => "#"}
                  %img.mw40{:alt => "holder-img", :src => "assets/img/avatars/3.jpg"}/
                .media-body.va-m
                  %h5.media-heading.mv5
                    Article
                    %small.text-muted - 08/16/22
                  Last Updated 36 days ago by
                  %a.text-system{:href => "#"} Max
            %li.p10.br-t.item-3
              .media
                %a.media-left{:href => "#"}
                  %img.mw40{:alt => "holder-img", :src => "assets/img/avatars/4.jpg"}/
                .media-body.va-m
                  %h5.media-heading.mv5
                    Article
                    %small.text-muted - 08/16/22
                  Last Updated 36 days ago by
                  %a.text-system{:href => "#"} Max
            %li.p10.br-t.item-4
              .media
                %a.media-left{:href => "#"}
                  %img.mw40{:alt => "holder-img", :src => "assets/img/avatars/5.jpg"}/
                .media-body.va-m
                  %h5.media-heading.mv5
                    Article
                    %small.text-muted - 08/16/22
                  Last Updated 36 days ago by
                  %a.text-system{:href => "#"} Max
        %li.dropdown
          %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
            %span.flag-xs.flag-us
            %span.fw600 US
          %ul.dropdown-menu.animated.animated-short.flipInX{:role => "menu"}
            %li
              %a.fw600{:href => "javascript:void(0);"}
                %span.flag-xs.flag-in.mr10
                Hindu
            %li
              %a.fw600{:href => "javascript:void(0);"}
                %span.flag-xs.flag-tr.mr10
                Turkish
            %li
              %a.fw600{:href => "javascript:void(0);"}
                %span.flag-xs.flag-es.mr10
                Spanish
        %li.ph10.pv20.hidden-xs
          %i.fa.fa-circle.text-tp.fs8
        %li.dropdown
          %a.dropdown-toggle.fw600.p15{"data-toggle" => "dropdown", :href => "#"}
            %img.mw30.br64.mr15{:alt => "avatar", :src => "assets/img/avatars/1.jpg"}/
            %span John.Smith
            %span.caret.caret-tp.hidden-xs
          %ul.dropdown-menu.dropdown-persist.pn.w250.bg-white{:role => "menu"}
            %li.bg-light.br-b.br-light.p8
              %span.pull-left.ml10
                %select#user-status{:style => "display: none;"}
                  %optgroup{:label => "Current Status:"}
                    %option{:value => "1-1"} Away
                    %option{:value => "1-2"} Offline
                    %option{:selected => "selected", :value => "1-3"} Online
                .btn-group{:style => "width: 100px;"}
                  %button.multiselect.dropdown-toggle.btn.btn-default.btn-sm{"data-toggle" => "dropdown", :style => "width: 100px;", :title => "Online", :type => "button"}
                    Online
                    %b.caret
                  %ul.multiselect-container.dropdown-menu
                    %li.multiselect-item.multiselect-group
                      %label Current Status:
                    %li
                      %a{:href => "javascript:void(0);"}
                        %label.radio
                          %input{:type => "radio", :value => "1-1"}/
                          Away
                    %li
                      %a{:href => "javascript:void(0);"}
                        %label.radio
                          %input{:type => "radio", :value => "1-2"}/
                          Offline
                    %li.active
                      %a{:href => "javascript:void(0);"}
                        %label.radio
                          %input{:type => "radio", :value => "1-3"}/
                          Online
              %span.pull-right.mr10
                %select#user-role{:style => "display: none;"}
                  %optgroup{:label => "Logged in As:"}
                    %option{:value => "1-1"} Client
                    %option{:value => "1-2"} Editor
                    %option{:selected => "selected", :value => "1-3"} Admin
                .btn-group{:style => "width: 100px;"}
                  %button.multiselect.dropdown-toggle.btn.btn-default.btn-sm{"data-toggle" => "dropdown", :style => "width: 100px;", :title => "Admin", :type => "button"}
                    Admin
                    %b.caret
                  %ul.multiselect-container.dropdown-menu.pull-right
                    %li.multiselect-item.multiselect-group
                      %label Logged in As:
                    %li
                      %a{:href => "javascript:void(0);"}
                        %label.radio
                          %input{:type => "radio", :value => "1-1"}/
                          Client
                    %li
                      %a{:href => "javascript:void(0);"}
                        %label.radio
                          %input{:type => "radio", :value => "1-2"}/
                          Editor
                    %li.active
                      %a{:href => "javascript:void(0);"}
                        %label.radio
                          %input{:type => "radio", :value => "1-3"}/
                          Admin
              .clearfix
            %li.of-h
              %a.fw600.p12.animated.animated-short.fadeInUp{:href => "#"}
                %span.fa.fa-envelope.pr5
                Messages
                %span.pull-right.lh20.h-20.label.label-warning.label-sm 2
            %li.br-t.of-h
              %a.fw600.p12.animated.animated-short.fadeInUp{:href => "#"}
                %span.fa.fa-user.pr5
                Friends
                %span.pull-right.lh20.h-20.label.label-warning.label-sm 6
            %li.br-t.of-h
              %a.fw600.p12.animated.animated-short.fadeInDown{:href => "#"}
                %span.fa.fa-gear.pr5
                Account Settings
            %li.br-t.of-h
              %a.fw600.p12.animated.animated-short.fadeInDown{:href => "#"}
                %span.fa.fa-power-off.pr5
                Logout
